{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@dtos/*": [
        "./src/dtos/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@screens/*": [
        "./src/screens/*"
      ],
      "@routes/*": [
        "./src/routes/*"
      ],
      "@global/*": [
        "./src/global/*"
      ]
    }
  },
}